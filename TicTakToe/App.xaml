<Application x:Class="TicTakToe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TicTakToe"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#656b3c</SolidColorBrush>
        <SolidColorBrush x:Key="LinesColor">#0a0a0a</SolidColorBrush>
        <SolidColorBrush x:Key="BoardColor">#6a785f</SolidColorBrush>
        <SolidColorBrush x:Key="FontColor">#d0d6d6</SolidColorBrush>
    </Application.Resources>
</Application>
